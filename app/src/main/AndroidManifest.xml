<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.weihuagu.websocket"
          xmlns:android="http://schemas.android.com/apk/res/android" >

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.weihuagu.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar" >
        <activity android:name="com.example.weihuagu.MainActivity" >

            <!-- <intent-filter > -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->

            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter > -->
        </activity >

        <service android:name=".MyService" />

        <activity android:name="com.example.weihuagu.activity.WebActivity" >
        </activity >
        <activity android:name=".WebSocketActivity" >
        </activity >
        <activity android:name="com.example.weihuagu.activity.ImageActivity" >
        </activity >
        <activity
            android:name="com.example.weihuagu.activity.ImageDragActivity"
            android:theme="@style/translucent" >
        </activity >
        <activity
            android:name="com.example.weihuagu.activity.PreviewVideoActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/translucent" >
        </activity >
        <activity
            android:name="com.example.weihuagu.activity.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Launcher" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter >
        </activity >
        <activity android:name="com.example.weihuagu.common.activity.ImagePagerActivity" >
        </activity >
        <activity android:name="com.example.weihuagu.activity.DiscoveryVideoActivity" >
        </activity >
        <activity android:name="com.example.weihuagu.activity.BlurActivity" >
        </activity >
        <activity android:name="com.example.weihuagu.BaseActivity" >
        </activity >
        <activity android:name="com.example.weihuagu.activity.ExtentBaseActivity" >
        </activity >
        <activity android:name="com.example.weihuagu.activity.AnimActivity" >
        </activity >
        <activity android:name="com.example.weihuagu.activity.ELMaActivity" >
        </activity >
        <activity android:name="com.example.weihuagu.activity.ELMaDetialsActivity" >
        </activity >
        <activity android:name="com.example.weihuagu.activity.SplashDemoActivity" >
        </activity >
        <activity android:name="com.example.weihuagu.activity.AllAnimActivity" >
        </activity >
        <activity android:name="com.example.weihuagu.activity.SearchAnimActivity" >
        </activity >
        <activity android:name="com.example.weihuagu.activity.SearchSecondActivity" >
        </activity >
    </application >

</manifest >